	<% 
	
	out.println("<table border = '1'>");
	out.println("<tr>");
	
	for(int i=2; i<10; i++){
		
			out.println("<th>" + i + "단</th>");
			
	}
	out.println("</tr>");
	
	for(int i=1; i<10; i++){
		out.println("<tr>");
		for (int j=2; j<10; j++){
		
		
		out.println("<td>" + j + "x" + i + "=" + (i*j) + "</th>");
	
		
		}	
		out.println("</tr>");
	}
	out.println("</table>");
	
	
	%>
	
	
